{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/forbid-prop-types": [
      1,
      {
        "forbid": [
          // "object", allow object
          "any"
          // "array"
        ]
      }
    ],
    "import/prefer-default-export": [
      0
    ],
    "class-methods-use-this": 0,
    "quotes": 0
  },
  "env": {
    "browser": true
  }
}
